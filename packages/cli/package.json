{
  "name": "@doom-ui/cli",
  "version": "0.3.6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doom-ui/doom-ui.git",
    "directory": "packages/cli"
  },
  "description": "CLI for Doom UI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "doom": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "bun scripts/copy-templates.ts && tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "clean": "rimraf dist templates"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "fs-extra": "^11.2.0",
    "@doom-ui/core": "^0.2.1",
    "@doom-ui/components": "^0.2.0"
  }
}
